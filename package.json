{
  "name": "woleet-gui",
  "version": "0.1.1",
  "private": true,
  "main": "electron.js",
  "scripts": {
    "ng": "ng",
    "start": "npm run electron:serve",
    "angular:buildandwatch": "ng build --watch",
    "build": "ng build",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "electron:serve": "npm run electon:TStoJS && npm-run-all -p angular:buildandwatch electron:serveandreload",
    "electron:serveandreload": "electron . --reload",
    "electon:TStoJS": "npx tsc --target 'ES5' --typeRoots 'node_modules/@type' --moduleResolution 'node' electron.ts",
    "electron:linux": "npm run build:prod && npx electron-builder build --linux",
    "electron:mac": "npm run build:prod && npx electron-builder build --mac",
    "electron:windows": "npm run build:prod && npx electron-builder build --windows"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.0",
    "@angular/animations": "^6.1.0",
    "@angular/cli": "~6.1.4",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "bootstrap": "^4.1.3",
    "codelyzer": "~4.2.1",
    "core-js": "^2.5.4",
    "electron": "^2.0.8",
    "electron-builder": "^20.28.3",
    "electron-reload": "^1.2.5",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "jquery": "^3.3.1",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "npm-run-all": "^4.1.3",
    "popper.js": "^1.14.4",
    "postinstall": "^0.4.1",
    "protractor": "~5.4.0",
    "rxjs": "^6.0.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2",
    "zone.js": "~0.8.26"
  }
}
