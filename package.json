{
  "name": "ProofKeeper",
  "version": "0.5.4",
  "private": true,
  "main": "electron.js",
  "scripts": {
    "ng": "npx ng",
    "start": "npm run electron:serve",
    "clean": "rm -rf release dist node_modules",
    "angular:buildandwatch": "ng build --watch",
    "build": "npm run electron:TStoJS && ng build",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "electron:serve": "npm run electron:TStoJS && rm -rf dist && npm-run-all -p angular:buildandwatch electron:serveandreload",
    "electron:serveandreload": "wait-on dist/ProofKeeper/index.html && electron . --liveenv",
    "electron:TStoJS": "npx tsc --target 'es2020' --module 'commonjs' --types 'node' --moduleResolution 'node' --lib 'DOM,es2015,es2016,es2017' electron.ts",
    "electron:linux": "npm run build:prod && npm run electron:linux-nobuild",
    "electron:linux-nobuild": "chmod +x dist/ProofKeeper/assets/bin/linux/woleet-cli && npx electron-builder build --linux",
    "electron:mac": "npm run build:prod && npm run electron:mac-nobuild",
    "electron:mac-nobuild": "chmod +x dist/ProofKeeper/assets/bin/darwin/woleet-cli && npx electron-builder build --mac",
    "electron:windows": "npm run build:prod && npm run electron:windows-nobuild",
    "electron:windows-nobuild": "npx electron-builder build --windows"
  },
  "dependencies": {
    "@electron/remote": "^2.0.8",
    "electron-store": "8.0.1"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "13.1.0",
    "@angular-devkit/build-angular": "13.3.3",
    "@angular/animations": "13.3.4",
    "@angular/cdk": "13.3.4",
    "@angular/cli": "13.3.3",
    "@angular/common": "13.3.4",
    "@angular/compiler": "13.3.4",
    "@angular/compiler-cli": "13.3.4",
    "@angular/core": "13.3.4",
    "@angular/forms": "13.3.4",
    "@angular/language-service": "13.3.4",
    "@angular/material": "13.3.4",
    "@angular/platform-browser": "13.3.4",
    "@angular/platform-browser-dynamic": "13.3.4",
    "@angular/router": "13.3.4",
    "@types/node": "17.0.29",
    "codelyzer": "^6.0.0",
    "core-js": "3.10.2",
    "electron": "18.1.0",
    "electron-builder": "23.0.3",
    "electron-notarize": "1.2.1",
    "electron-reload": "1.5.0",
    "electron-settings": "4.0.2",
    "hammerjs": "2.0.8",
    "loglevel": "1.8.0",
    "material-design-icons": "3.0.1",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "npm-run-all": "4.1.5",
    "rxjs": "6.6.7",
    "semver": "7.3.7",
    "ts-node": "10.7.0",
    "tslib": "^2.0.0",
    "tslint": "~6.1.0",
    "typescript": "4.6.3",
    "wait-on": "6.0.1",
    "zone.js": "~0.11.4"
  }
}
