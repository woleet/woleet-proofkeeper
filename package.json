{
  "name": "ProofKeeper",
  "version": "0.5.3",
  "private": true,
  "main": "electron.js",
  "scripts": {
    "ng": "npx ng",
    "start": "npm run electron:serve",
    "clean": "rm -rf release dist node_modules",
    "postinstall": "node build/postinstall.js",
    "angular:buildandwatch": "ng build --watch",
    "build": "npm run electron:TStoJS && ng build",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "electron:serve": "npm run electron:TStoJS && rm -rf dist && npm-run-all -p angular:buildandwatch electron:serveandreload",
    "electron:serveandreload": "wait-on dist/ProofKeeper/index.html && electron . --liveenv",
    "electron:TStoJS": "npm run postinstall && npx tsc --target 'es5' --types 'node' --moduleResolution 'node' --lib 'DOM,es2015,es2016,es2017' electron.ts",
    "electron:linux": "npm run build:prod && npm run electron:linux-nobuild",
    "electron:linux-nobuild": "chmod +x dist/ProofKeeper/assets/bin/linux/woleet-cli && npx electron-builder build --linux",
    "electron:mac": "npm run build:prod && npm run electron:mac-nobuild",
    "electron:mac-nobuild": "chmod +x dist/ProofKeeper/assets/bin/darwin/woleet-cli && npx electron-builder build --mac",
    "electron:windows": "npm run build:prod && npm run electron:windows-nobuild",
    "electron:windows-nobuild": "npx electron-builder build --windows"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.901.15",
    "@angular/animations": "9.1.13",
    "@angular/cdk": "9.2.4",
    "@angular/cli": "9.1.15",
    "@angular/common": "9.1.13",
    "@angular/compiler": "9.1.13",
    "@angular/compiler-cli": "9.1.13",
    "@angular/core": "9.1.13",
    "@angular/forms": "9.1.13",
    "@angular/language-service": "9.1.13",
    "@angular/material": "9.2.4",
    "@angular/platform-browser": "9.1.13",
    "@angular/platform-browser-dynamic": "9.1.13",
    "@angular/router": "9.1.13",
    "@types/node": "12.20.10",
    "codelyzer": "5.2.2",
    "core-js": "3.10.2",
    "electron": "9.4.4",
    "electron-builder": "23.0.3",
    "electron-notarize": "1.2.1",
    "electron-reload": "1.5.0",
    "electron-settings": "3.2.0",
    "electron-store": "5.2.0",
    "hammerjs": "2.0.8",
    "loglevel": "1.7.1",
    "material-design-icons": "3.0.1",
    "npm-run-all": "4.1.5",
    "rxjs": "6.6.7",
    "semver": "7.3.5",
    "ts-node": "8.10.2",
    "tslib": "1.14.1",
    "tslint": "6.1.3",
    "typescript": "3.8.3",
    "wait-on": "5.3.0",
    "zone.js": "0.10.3"
  }
}
